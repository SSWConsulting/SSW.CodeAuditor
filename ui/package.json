{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup -c -w --bundleConfigAsCjs",
    "predeploy": "npm run build",
    "deploy": "firebase deploy --token $FIREBASETOKEN --only hosting",
    "login:ci": "firebase login:ci",
    "count": "npx cloc --force-lang=\"HTML\",svelte src",
    "start": "sirv public --single -p 4200"
  },
  "engines": {
    "node": "20"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "postcss": "^8.4.38",
    "rollup": "^3.29.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.2.2",
    "sass": "^1.77.6",
    "svelte": "^3.59.2",
    "tailwindcss": "^3.4.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "chart.js": "^4.4.3",
    "codemirror": "^5.65.16",
    "date-fns": "^2.30.0",
    "export-to-csv": "^1.3.0",
    "firebase": "^10.12.2",
    "firebase-tools": "^13.11.2",
    "github-markdown-css": "^5.6.1",
    "js-beautify": "^1.15.1",
    "lighthouse-viewer": "^0.1.235",
    "minimatch": "^9.0.4",
    "pace-js": "^1.2.4",
    "query-string": "^8.2.0",
    "ramda": "^0.29.1",
    "sirv-cli": "^2.0.2",
    "slug": "^8.2.3",
    "svelte-navigator": "^3.2.2",
    "svelte-preprocess": "^5.1.4",
    "svelte-router-spa": "^7.0.1"
  }
}
