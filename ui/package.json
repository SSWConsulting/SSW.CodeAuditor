{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c && cp public/index.html public/200.html",
    "dev": "rollup -c -w",
    "predeploy": "npm run build && cp public/index.html public/200.html",
    "deploy": "firebase deploy --token $FIREBASETOKEN --only hosting",
    "login:ci": "firebase login:ci",
    "count": "npx cloc --force-lang=\"HTML\",svelte src",
    "start": "sirv public --single"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.4.1",
    "dotenv": "^8.2.0",
    "node-sass": "^4.14.1",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^7.0.0",
    "surge": "^0.21.6",
    "svelte": "^3.24.1"
  },
  "dependencies": {
    "@sveltejs/pancake": "0.0.17",
    "chart.js": "^2.9.4",
    "date-fns": "^2.12.0",
    "export-to-csv": "^0.2.1",
    "firebase": "^7.22.0",
    "firebase-tools": "^8.7.0",
    "frappe-charts": "^1.5.2",
    "fs": "0.0.1-security",
    "fusioncharts": "^3.15.2",
    "lighthouse": "^6.2.0",
    "query-string": "^6.12.1",
    "ramda": "^0.27.0",
    "rollup-plugin-json": "^4.0.0",
    "sirv-cli": "^0.4.4",
    "slug": "^2.1.1",
    "svelte-frappe-charts": "^1.2.2",
    "svelte-fusioncharts": "^1.0.0",
    "svelte-preprocess": "^3.7.4",
    "svelte-router-spa": "^5.5.0",
    "tailwindcss": "^1.9.6"
  }
}
