{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup -c -w --bundleConfigAsCjs",
    "predeploy": "npm run build",
    "deploy": "firebase deploy --token $FIREBASETOKEN --only hosting",
    "login:ci": "firebase login:ci",
    "count": "npx cloc --force-lang=\"HTML\",svelte src",
    "start": "sirv public --single -p 4200"
  },
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "postcss": "^8.4.31",
    "rollup": "^3.29.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.6",
    "sass": "^1.69.5",
    "svelte": "^3.59.1",
    "tailwindcss": "^3.3.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "chart.js": "^4.4.0",
    "codemirror": "^5.65.15",
    "date-fns": "^2.30.0",
    "export-to-csv": "^1.2.1",
    "firebase": "^10.6.0",
    "firebase-tools": "^13.0.3",
    "github-markdown-css": "^5.4.0",
    "js-beautify": "^1.14.11",
    "lighthouse-viewer": "^0.1.235",
    "minimatch": "^9.0.3",
    "pace-js": "^1.0.2",
    "query-string": "^8.1.0",
    "ramda": "^0.29.1",
    "sirv-cli": "^2.0.2",
    "slug": "^8.2.3",
    "svelte-navigator": "^3.2.2",
    "svelte-preprocess": "^5.1.1",
    "svelte-router-spa": "^7.0.1"
  }
}
